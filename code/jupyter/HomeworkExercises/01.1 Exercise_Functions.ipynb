    "# import math functions\n",
    "import math\n",
    "\n",
    "# defining the solver function\n",
    "def quadratic(a,b,c):\n",
    "    \n",
    "    p = -b/2./a\n",
    "    q = math.sqrt(b*b - 4*a*c)/2./a\n",
    "    \n",
    "    x1 = p+q\n",
    "    x2 = p-q\n",
    "    \n",
    "    return (x1,x2)\n",
    "\n",
    "# demo the function\n",
    "sol = quadratic(1.,3.,2.)\n",
    "print(\"sol = \",*sol)"
